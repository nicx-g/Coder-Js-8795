{
  "author": "Cripto Coins <nicx_g@hotmail.com>",
  "description": "No sé qué poner acá :) sólo sé que no voy a tocar más el css",
  "license": "MIT",
  "name": "sass-v1-0",
  "scripts": {
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
    "build-css": "npm run css && npm run css-autoprefixer",
    "css": "cross-var node-sass --importer node_modules/node-sass-glob-importer/dist/cli.js --output-style compressed src/scss/main.scss src/css/styles.css",
    "css-autoprefixer": "cross-var \"postcss -u autoprefixer -r src/css/*\""
  },
  "version": "1.0.0",
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "cross-var": "^1.1.0",
    "node-sass": "^4.14.1",
    "node-sass-glob-importer": "^5.3.2",
    "postcss-cli": "^7.1.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.3",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.5.2",
    "jquery": "^3.5.1",
    "uglify-js": "^3.11.0"
  }
}
